PROBLE:https://practice.geeksforgeeks.org/problems/count-pairs-with-given-sum5022/1#
VIDEO:https://youtu.be/ijKmiFqjzi4
//BRUTE FORCE(GETTING TLE)
TIME:O(N2)
SPACE:O(1)
class Solution{   
public:
    int getPairsCount(int arr[], int n, int k) {
        // code here
       int count =0;
       for(int i=0;i<n-1;i++){
           for(int j=i+1;j<n;j++){
               if(arr[i]+arr[j]==k){
                   count++;
               }
           }
       }
       return count;
    }
};
