problem:https://leetcode.com/problems/maximum-product-subarray/
video:https://youtu.be/vtJvbRlHqTA
/*
brute force:
time:O(n*3)
space:O(1)
*/

#include<bits/stdc++.h>
using namespace std;
class Solution {
public:
    int maxProduct(vector<int>& nums) {
    int ans=INT_MIN;
        int pro;
        for(int i=0;i<nums.size();i++){
            for(int j=i;j<nums.size();j++){
                 pro=1;
               for(int k=i;k<=j;k++){
               

                    pro=pro*nums[k];
                    
               }
                ans=max(ans,pro);
            }
        }
        return ans;
    }
};
